<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane maxHeight="600.0" maxWidth="600.0" minHeight="600.0" minWidth="600.0" prefHeight="600.0" prefWidth="600.0"
         tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="Table">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <GridPane prefHeight="570.0" prefWidth="600.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="550.0" minHeight="0.0" prefHeight="25.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <VBox prefHeight="550.0" prefWidth="600.0" GridPane.rowSpan="1">
                                    <children>
                                        <TreeTableView fx:id="treeTableGeneticAlg" prefHeight="550.0" prefWidth="600.0"/>
                                    </children>
                                </VBox>
                                <HBox prefHeight="25.0" prefWidth="600.0" GridPane.rowIndex="1" GridPane.rowSpan="1">
                                    <children>
                                        <Button fx:id="start" mnemonicParsing="false" onAction="#startOnAction"
                                                prefHeight="25.0" prefWidth="52.0" text="Start"/>
                                        <Slider fx:id="timeLine" prefHeight="25.0" prefWidth="481.0" onScroll="#onScrollTime"/>
                                        <TextField fx:id="tick" editable="false" prefHeight="25.0" prefWidth="66.0" text="0"/>
                                    </children>
                                </HBox>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
